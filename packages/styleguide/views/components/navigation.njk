<ul class="fr-nav__items" :data-level="depth">
  <li v-for="item in items">
    <app-link :to="item.url" v-if="item.url" class="fr-nav__label fr-nav__link" v-html="item.label"></app-link>
    <span class="fr-nav__label fr-nav__toggle" v-html="item.label" v-else></span>
    <navigation :pages="item.children" :depth="depth + 1" v-if="item.children"></navigation>
  </li>
</ul>
