  <div class="fr-inspector__preview">
    <iframe :srcdoc="preview" :src="variant.previewUrl" seamless frameborder="0" class="fr-preview" ref="preview"></iframe>
  </div>

  <div class="fr-inspector__info">
    <ul class="fr-tabs fr-inspector__tabs">
      <li class="fr-tabs__tab" :class="{ 'is-current': currentTab === i }" v-for="(panel, i) in panels">
        <a href="#" @click="selectTab(i)">{{ panel.label }}</a>
      </li>
    </ul>
    <div class="fr-panels fr-inspector__panels">
      <div
        class="fr-panels__panel"
        v-show="currentTab === i"
        v-for="(panel, i) in panels"
        v-html="panel.content">
      </div>
    </div>
  </div>
</div>

