{
  "name": "@fractalite/theme-styleguide",
  "version": "0.0.0",
  "scripts": {
    "build": "npm run copy-assets && parcel build ./assets/styleguide.js --no-source-maps",
    "dev": "npm run copy-assets && parcel watch ./assets/styleguide.js",
    "copy-assets": "mkdir -p ./dist && cp ./node_modules/iframe-resizer/js/iframeResizer.contentWindow.js ./dist/iframe.js"
  },
  "dependencies": {
    "@fractalite/support": "^0.0.0",
    "iframe-resizer": "^3.5.15",
    "jquery": "^3.3.1",
    "normalize.css": "^8.0.0"
  },
  "devDependencies": {
    "parcel-bundler": "^1.10.1",
    "sass": "^1.14.1"
  }
}
